<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kindergarten.teacher.mapper.TeacherMapper">
    
    <select id="login" resultType="com.kindergarten.teacher.model.TblTeachers"
    parameterType="com.kindergarten.teacher.model.TblTeachers">
     select * from tbl_teachers where teacher_tel=#{teacherTel} and teacher_pwd=#{teacherPwd}
    </select>

</mapper>